{
  "name": "jss-syntax-highlighting",
  "version": "0.0.1",
  "type": "module",
  "description": "Syntax highlighting for the JSS framework",
  "engines": {
    "vscode": "^1.0.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "jss",
        "aliases": [
          "JSS",
          "jss"
        ],
        "extensions": [
          ".jss"
        ],
        "configuration": "./js-syntax.language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jss",
        "scopeName": "source.js-syntax",
        "path": "./js-syntax.tmLanguage.json"
      }
    ]
  }
}
